    "from sklearn.datasets import load_iris\n",
    "from sklearn.cluster import KMeans\n",
    "\n",
    "data = load_iris()\n",
    "model = KMeans(n_clusters=3)\n",
    "model.fit(data.data)\n",
    "\n",
    "# 数组的每一行代表一个簇的中心点\n",
    "print(model.cluster_centers_)"
